 var table = $('#table').DataTable();
 
// #column3_search is a <input type="text"> element
$('#suche_inaktiv').on( 'click', function () {
    table
        .columns( 1 )
        .search( "1")
        .draw();
} );
$('#suche_abgelaufen').on( 'click', function () {
    table
        .columns( 1 )
        .search( "2")
        .draw();
} );

$('#suche_baldabgelaufen').on( 'click', function () {
    table
        .columns( 1 )
        .search( "3")
        .draw();
} );
$('#suche_alle').on( 'click', function () {
    table
        .columns( 1 )
        .search( "")
        .draw();
} );

   } );
   



